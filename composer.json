{
    "name": "jtl/connector",
    "type": "library",
    "description": "JTL-Connector protocol library and classes",
    "keywords": ["jtl", "connector", "protocol", "model", "data"],
    "homepage": "http://www.jtl-software.de",
    "license": "MIT",
    "authors": [{
        "name": "JTL Software GmbH",
